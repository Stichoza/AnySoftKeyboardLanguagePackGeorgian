	<SequenceMapping keySequence="KEYCODE_Z" shiftModifier="true" targetChar="ძ"/>
	<SequenceMapping keySequence="KEYCODE_C" shiftModifier="true" targetChar="ჩ"/>
	<!-- Old Georgian chars -->
	<SequenceMapping keySequence="KEYCODE_Y" shiftModifier="true" targetChar="ჱ"/>
	<SequenceMapping keySequence="KEYCODE_U" shiftModifier="true" targetChar="ჲ"/>
	<SequenceMapping keySequence="KEYCODE_I" shiftModifier="true" targetChar="ჳ"/>
